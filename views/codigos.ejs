        <script>

            ListarCodigo();

            function ListarCodigo(){

                $.ajax({
                    type: "GET",
                    url: "/ListarCodigos",
                    success: function (res) {

                        var table = $('#tbCodigo').DataTable();
                        table.destroy();
                        $('#tbCodigo').empty();
                        table = $('#tbCodigo').DataTable({
                            "ordering": false,
                            pageLength: 10,
                            responsive: true,
                            dom: '<"html5buttons"B>lTfgitp',
                            buttons: [
                            { extend: 'csv', className: 'exportar-csv-e' },
                            { extend: 'excel', title: 'key-search-excel', className: 'exportar-excel-e' },
                            {
                                text: '<i class=\'fa fa-refresh\'><i>',
                                action: function (e, dt, node, config) {
                                    location.reload();
                                }
                            }
                            ],
                            data: res,
                            columns: [
                            { data: 'aut_id', title: "Código" },
                            { data: 'aut_fecha', title: "Fecha Registro" },
                            { data: 'aut_empresa', title: "Empresa Receptora" },
                            { data: 'aut_serie', title: "Clave Secreta" },
                            { data: 'aut_mac', title: "MAC" },
                            { data: 'aut_codigo', title: "Código" },
                            { data: 'aut_estado', title: "Estado" }
                            ],
                                               "columnDefs": [
                        {
                            "targets": [0],
                            "visible": false,
                            "searchable": false
                        }],
                            "language": {
                                "sProcessing": "Procesando...",
                                "sLengthMenu": "Mostrar _MENU_ registros",
                                "sZeroRecords": "No se encontraron resultados",
                                "sEmptyTable": "Ningún dato disponible en esta tabla",
                                "sInfo": "Mostrando registros del _START_ al _END_ de un total de _TOTAL_ registros",
                                "sInfoEmpty": "Mostrando registros del 0 al 0 de un total de 0 registros",
                                "sInfoFiltered": "(filtrado de un total de _MAX_ registros)",
                                "sInfoPostFix": "",
                                "sSearch": "",
                                "sUrl": "",
                                "sInfoThousands": ",",
                                "sLoadingRecords": "Cargando...",
                                "oPaginate": {
                                    "sFirst": "Primero",
                                    "sLast": "Último",
                                    "sNext": "Siguiente",
                                    "sPrevious": "Anterior"
                                },
                                "oAria": {
                                    "sSortAscending": ": Activar para ordenar la columna de manera ascendente",
                                    "sSortDescending": ": Activar para ordenar la columna de manera descendente"
                                },
                                searchPlaceholder: "Buscar datos"
                            }
                        });
                    }
                });
            }

        </script>

